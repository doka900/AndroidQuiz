<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.AsocijacijeActivity">
    <include
        android:id="@+id/resultbar"
        layout="@layout/resultbar" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="Asocijacije"
            android:layout_marginTop="55dp"
            android:textAlignment="center"
            android:textColor="@color/golden"
            android:textSize="25sp" />

        <androidx.cardview.widget.CardView
            android:id="@+id/asocijacija_A1"
            android:layout_width="180dp"
            android:layout_height="40dp"
            android:layout_marginTop="130dp"
            card_view:cardCornerRadius="20dp"
            card_view:cardBackgroundColor="@color/main_green"
            android:layout_marginLeft="15dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="Asocijacija A1"
                android:layout_marginHorizontal="10dp"
                android:layout_marginVertical="5dp"
                android:textAlignment="center"
                android:textColor="@color/golden"
                android:textSize="25sp" />
        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
            android:id="@+id/asocijacija_A2"
            android:layout_width="180dp"
            android:layout_height="40dp"
            android:layout_marginTop="5dp"
            card_view:cardCornerRadius="20dp"
            card_view:cardBackgroundColor="@color/main_green"
            android:layout_below="@+id/asocijacija_A1"
            android:layout_alignLeft="@+id/asocijacija_A1">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="Asocijacija A2"
                android:layout_marginHorizontal="10dp"
                android:layout_marginVertical="5dp"
                android:textAlignment="center"
                android:textColor="@color/golden"
                android:textSize="25sp" />
        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
            android:id="@+id/asocijacija_A3"
            android:layout_width="180dp"
            android:layout_height="40dp"
            android:layout_marginTop="5dp"
            card_view:cardCornerRadius="20dp"
            card_view:cardBackgroundColor="@color/main_green"
            android:layout_below="@+id/asocijacija_A2"
            android:layout_alignLeft="@+id/asocijacija_A2">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="Asocijacija A3"
                android:layout_marginHorizontal="10dp"
                android:layout_marginVertical="5dp"
                android:textAlignment="center"
                android:textColor="@color/golden"
                android:textSize="25sp" />
        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
            android:id="@+id/asocijacija_A4"
            android:layout_width="180dp"
            android:layout_height="40dp"
            android:layout_marginTop="5dp"
            card_view:cardCornerRadius="20dp"
            card_view:cardBackgroundColor="@color/main_green"
            android:layout_below="@+id/asocijacija_A3"
            android:layout_alignLeft="@+id/asocijacija_A3">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="Asocijacija A4"
                android:layout_marginHorizontal="10dp"
                android:layout_marginVertical="5dp"
                android:textAlignment="center"
                android:textColor="@color/golden"
                android:textSize="25sp" />
        </androidx.cardview.widget.CardView>



        <androidx.cardview.widget.CardView
            android:id="@+id/asocijacija_B1"
            android:layout_width="180dp"
            android:layout_height="40dp"
            android:layout_marginTop="130dp"
            android:layout_marginRight="15dp"
            card_view:cardCornerRadius="20dp"
            card_view:cardBackgroundColor="@color/main_green"
            android:layout_alignParentRight="true">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="Asocijacija B1"
                android:layout_marginHorizontal="10dp"
                android:layout_marginVertical="5dp"
                android:textAlignment="center"
                android:textColor="@color/golden"
                android:textSize="25sp" />
        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
            android:id="@+id/asocijacija_B2"
            android:layout_width="180dp"
            android:layout_height="40dp"
            android:layout_marginTop="5dp"
            card_view:cardCornerRadius="20dp"
            card_view:cardBackgroundColor="@color/main_green"
            android:layout_below="@+id/asocijacija_B1"
            android:layout_alignLeft="@+id/asocijacija_B1">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="Asocijacija B2"
                android:layout_marginHorizontal="10dp"
                android:layout_marginVertical="5dp"
                android:textAlignment="center"
                android:textColor="@color/golden"
                android:textSize="25sp" />
        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
            android:id="@+id/asocijacija_B3"
            android:layout_width="180dp"
            android:layout_height="40dp"
            android:layout_marginTop="5dp"
            card_view:cardCornerRadius="20dp"
            card_view:cardBackgroundColor="@color/main_green"
            android:layout_below="@+id/asocijacija_B2"
            android:layout_alignLeft="@+id/asocijacija_B2">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="Asocijacija B3"
                android:layout_marginHorizontal="10dp"
                android:layout_marginVertical="5dp"
                android:textAlignment="center"
                android:textColor="@color/golden"
                android:textSize="25sp" />
        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
            android:id="@+id/asocijacija_B4"
            android:layout_width="180dp"
            android:layout_height="40dp"
            android:layout_marginTop="5dp"
            card_view:cardCornerRadius="20dp"
            card_view:cardBackgroundColor="@color/main_green"
            android:layout_below="@+id/asocijacija_B3"
            android:layout_alignLeft="@+id/asocijacija_B3">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="Asocijacija B4"
                android:layout_marginHorizontal="10dp"
                android:layout_marginVertical="5dp"
                android:textAlignment="center"
                android:textColor="@color/golden"
                android:textSize="25sp" />
        </androidx.cardview.widget.CardView>


        <androidx.cardview.widget.CardView
            android:id="@+id/asocijacija_final"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginVertical="20dp"
            android:layout_marginHorizontal="15dp"
            card_view:cardCornerRadius="20dp"
            card_view:cardBackgroundColor="@color/main_green"
            android:layout_below="@+id/asocijacija_A4">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="Asocijacija Final"
                android:layout_marginHorizontal="10dp"
                android:layout_marginVertical="5dp"
                android:textAlignment="center"
                android:textColor="@color/golden"
                android:textSize="25sp" />
        </androidx.cardview.widget.CardView>



        <androidx.cardview.widget.CardView
            android:id="@+id/asocijacija_C1"
            android:layout_width="180dp"
            android:layout_height="40dp"
            android:layout_below="@+id/asocijacija_final"
            android:layout_marginLeft="15dp"
            card_view:cardCornerRadius="20dp"
            card_view:cardBackgroundColor="@color/main_green">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="Asocijacija C1"
                android:layout_marginHorizontal="10dp"
                android:layout_marginVertical="5dp"
                android:textAlignment="center"
                android:textColor="@color/golden"
