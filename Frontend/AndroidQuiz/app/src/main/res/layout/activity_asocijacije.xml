        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
            android:id="@+id/asocijacija_C2"
            android:layout_width="180dp"
            android:layout_height="40dp"
            android:layout_marginTop="5dp"
            card_view:cardCornerRadius="20dp"
            card_view:cardBackgroundColor="@color/main_green"
            android:layout_below="@+id/asocijacija_C1"
            android:layout_alignLeft="@+id/asocijacija_C1">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="Asocijacija C2"
                android:layout_marginHorizontal="10dp"
                android:layout_marginVertical="5dp"
                android:textAlignment="center"
                android:textColor="@color/golden"
                android:textSize="25sp" />
        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
            android:id="@+id/asocijacija_C3"
            android:layout_width="180dp"
            android:layout_height="40dp"
            android:layout_marginTop="5dp"
            card_view:cardCornerRadius="20dp"
            card_view:cardBackgroundColor="@color/main_green"
            android:layout_below="@+id/asocijacija_C2"
            android:layout_alignLeft="@+id/asocijacija_C2">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="Asocijacija C3"
                android:layout_marginHorizontal="10dp"
                android:layout_marginVertical="5dp"
                android:textAlignment="center"
                android:textColor="@color/golden"
                android:textSize="25sp" />
        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
            android:id="@+id/asocijacija_C4"
            android:layout_width="180dp"
            android:layout_height="40dp"
            android:layout_marginTop="5dp"
            card_view:cardCornerRadius="20dp"
            card_view:cardBackgroundColor="@color/main_green"
            android:layout_below="@+id/asocijacija_C3"
            android:layout_alignLeft="@+id/asocijacija_C3">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="Asocijacija C4"
                android:layout_marginHorizontal="10dp"
                android:layout_marginVertical="5dp"
                android:textAlignment="center"
                android:textColor="@color/golden"
                android:textSize="25sp" />
        </androidx.cardview.widget.CardView>



        <androidx.cardview.widget.CardView
            android:id="@+id/asocijacija_D1"
            android:layout_width="180dp"
            android:layout_height="40dp"
            android:layout_marginRight="15dp"
            card_view:cardCornerRadius="20dp"
            card_view:cardBackgroundColor="@color/main_green"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/asocijacija_final">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="Asocijacija D1"
                android:layout_marginHorizontal="10dp"
                android:layout_marginVertical="5dp"
                android:textAlignment="center"
                android:textColor="@color/golden"
                android:textSize="25sp" />
        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
            android:id="@+id/asocijacija_D2"
            android:layout_width="180dp"
            android:layout_height="40dp"
            android:layout_marginTop="5dp"
            card_view:cardCornerRadius="20dp"
            card_view:cardBackgroundColor="@color/main_green"
            android:layout_below="@+id/asocijacija_D1"
            android:layout_alignLeft="@+id/asocijacija_D1">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="Asocijacija D2"
                android:layout_marginHorizontal="10dp"
                android:layout_marginVertical="5dp"
                android:textAlignment="center"
                android:textColor="@color/golden"
                android:textSize="25sp" />
        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
            android:id="@+id/asocijacija_D3"
            android:layout_width="180dp"
            android:layout_height="40dp"
            android:layout_marginTop="5dp"
            card_view:cardCornerRadius="20dp"
            card_view:cardBackgroundColor="@color/main_green"
            android:layout_below="@+id/asocijacija_D2"
            android:layout_alignLeft="@+id/asocijacija_D2">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="Asocijacija D3"
                android:layout_marginHorizontal="10dp"
                android:layout_marginVertical="5dp"
                android:textAlignment="center"
                android:textColor="@color/golden"
                android:textSize="25sp" />
        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
            android:id="@+id/asocijacija_D4"
            android:layout_width="180dp"
            android:layout_height="40dp"
            android:layout_marginTop="5dp"
            card_view:cardCornerRadius="20dp"
            card_view:cardBackgroundColor="@color/main_green"
            android:layout_below="@+id/asocijacija_D3"
            android:layout_alignLeft="@+id/asocijacija_D3">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="Asocijacija D4"
                android:layout_marginHorizontal="10dp"
                android:layout_marginVertical="5dp"
                android:textAlignment="center"
                android:textColor="@color/golden"
                android:textSize="25sp" />
        </androidx.cardview.widget.CardView>


    </RelativeLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="bottom|center">
        <include
            android:id="@+id/gamebar"
            layout="@layout/gamebar" />
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>